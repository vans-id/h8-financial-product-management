import io.swagger.v3.oas.annotations.info.Info;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@OpenAPIDefinition(
        info = @Info(
                title = "Financial Product Management Application",
                description = "Financial Product Management Open API",
                version = "v1.1"
        )
)


@SpringBootApplication
public class FinancialProductManagementApplication {

    public static void main(String[] args) {
        SpringApplication.run(FinancialProductManagementApplication.class, args);
    }

}
